<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="300dip"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:background="#FFFFFF"
              android:orientation="vertical">

    <TextView
            android:id="@+id/dia_starttable_title"
            android:layout_width="match_parent"
            android:layout_height="30dip"
            android:background="@drawable/dialog_title"
            android:gravity="center"/>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/dialog_main"
            android:orientation="vertical">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="30dip"
                    android:paddingRight="30dip"
                    android:paddingTop="20dip">

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="@string/manNum"/>

                <EditText
                        android:id="@+id/diastartbl_et_malecounts"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dip"
                        android:background="@drawable/dia_editor_n"/>
            </LinearLayout>

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="30dip"
                    android:paddingRight="30dip"
                    android:paddingTop="20dip">

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="@string/womanNum"/>

                <EditText
                        android:id="@+id/diastartbl_et_femalecounts"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dip"
                        android:background="@drawable/dia_editor_n"/>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="15dip"
                android:paddingRight="30dip"
                android:paddingTop="20dip">

            <CheckBox
                    android:id="@+id/diastartbl_cb_vip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="@string/isVip"/>
        </LinearLayout>

        <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:orientation="horizontal"
                android:paddingTop="20dip">

            <Button
                    android:id="@+id/dia_cancelbtn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/dia_btn_selector"
                    android:text="@string/cancle"/>

            <Button
                    android:id="@+id/dia_certainbtn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:background="@drawable/dia_btn_selector"
                    android:text="@string/confirm"/>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>