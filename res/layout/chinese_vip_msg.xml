<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="#FFFFFF"
              android:orientation="vertical">
    <include layout="@layout/chinese_vip_activity_order_top"
             android:clickable="false"/>
    <LinearLayout
            android:id="@+id/vip_layout2"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="left|center"
            android:orientation="horizontal">

        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:textColor="#000000"
                android:text="@string/vip_yingFText"/>

        <TextView
                android:id="@+id/chinese_vipmsg_yingFText1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:text=""/>

    </LinearLayout>
    <LinearLayout
            android:id="@+id/vip_layout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:baselineAligned="true"
            android:padding="5dp"
            android:orientation="vertical">

        <LinearLayout android:orientation="horizontal"
                      android:layout_width="fill_parent"
                      android:layout_height="fill_parent"
                      android:id="@+id/linearLayout">
            <TextView android:layout_height="wrap_content"
                      android:layout_width="0dp"
                      android:layout_weight="1"
                      android:textColor="#000000"
                      android:gravity="right"
                      android:padding="5dp"
                      android:text="@string/vip_num"/>
            <TextView android:layout_height="wrap_content"
                      android:layout_width="0dp"
                      android:layout_weight="1"
                      android:textColor="#000000"
                      android:padding="5dp"
                      android:id="@+id/chinese_vipmsg_number"/>
            <TextView android:layout_height="wrap_content"
                      android:layout_width="0dp"
                      android:layout_weight="1"
                      android:gravity="right"
                      android:textColor="#000000"
                      android:text="@string/vip_chuXuKText"/>
            <TextView android:layout_height="wrap_content"
                      android:layout_width="0dp"
                      android:layout_weight="1"
                      android:textColor="#000000"
                      android:padding="5dp"
                      android:id="@+id/chinese_vipmsg_yue"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:weightSum="4">
            <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:padding="5dp"
                    android:gravity="right"
                    android:textColor="#000000"
                    android:text="@string/vip_jiFenYEText"/>
            <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:textColor="#000000"
                    android:padding="5dp"
                    android:id="@+id/chinese_vipmsg_jifen_yue"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
            android:id="@+id/vip_layout6"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:orientation="horizontal">

        <TextView
                android:id="@+id/vip_quanXinXText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:text="@string/vip_quanXinXText"/>

    </LinearLayout>

    <LinearLayout
            android:id="@+id/vip_layout7"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="vertical">
        <LinearLayout
                android:id="@+id/Vip_couponListViewTitle"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:gravity="center"
                android:orientation="horizontal">

            <TextView
                    android:id="@+id/vip_ticketIdTitle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:gravity="center"
                    android:textColor="#000000"
                    android:text="@string/vip_ticketId"/>

            <TextView
                    android:id="@+id/vip_ticketNameTitle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:textColor="#000000"
                    android:text="@string/vip_ticketName"/>

            <TextView
                    android:id="@+id/vip_ticketMoneyTitle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:textColor="#000000"
                    android:text="@string/vip_ticketMoney"/>

        </LinearLayout>
        <ListView
                android:id="@+id/chinese_vipmsg_couponListView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:cacheColorHint="#00000000"
                >
        </ListView>
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:layout_margin="10dp"
            android:id="@+id/vip_layout8">
        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/confirm"
                android:background="@drawable/but_alone_bg"
                android:paddingLeft="30dp"
                android:paddingRight="30dp"
                android:layout_marginRight="10dp"
                android:id="@+id/chinese_vipmsg_confirm"/>
        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/cancle"
                android:background="@drawable/but_alone_bg"
                android:paddingLeft="30dp"
                android:paddingRight="30dp"
                android:layout_marginRight="10dp"
                android:id="@+id/chinese_vipmsg_cancel"/>
    </LinearLayout>

</LinearLayout>
