<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <LinearLayout
            android:id="@+id/vip_layout2"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="left|center"
            android:orientation="horizontal">

        <TextView
                android:id="@+id/vip_yingFText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="@string/vip_yingFText"/>

        <TextView
                android:id="@+id/vip_yingFText1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""/>

    </LinearLayout>
    <!-- Three start -->

    <LinearLayout
            android:id="@+id/vip_layout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:baselineAligned="true"
            android:orientation="horizontal">

        <LinearLayout
                android:id="@+id/vip_layout3_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:orientation="vertical">

            <TextView
                    android:id="@+id/vip_VIPNumText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                    android:text="@string/vip_VIPNumText"/>
            <TextView
                    android:id="@+id/vip_quanYEText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="15dp"
                    android:text="@string/vip_quanYEText"/>
            <TextView
                    android:id="@+id/vip_jiFenYEText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="15dp"
                    android:text="@string/vip_jiFenYEText"/>
            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:text="@string/vip_chuXuKText"/>
            <TextView
                android:id="@+id/vip_quanKeYYEText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:text="@string/vip_quanKeYYEText"/>

        </LinearLayout>

        <LinearLayout
                android:id="@+id/vip_layout3_2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:orientation="vertical">

            <EditText
                    android:id="@+id/vip_VIPNumEdut"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="text"
                    android:enabled="false"
                    android:ems="10"/>

            <EditText
                    android:id="@+id/vip_quanYEEdit"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="text"
                    android:enabled="false"
                    android:ems="10"/>
            <EditText
                    android:id="@+id/vip_jiFenYEEdit"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="text"
                    android:enabled="false"
                    android:ems="10"/>
            <EditText
                android:id="@+id/vip_chuXuKEdit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="text"
                android:enabled="false"
                android:ems="10"/>

            <EditText
                android:id="@+id/vip_quanKeYYEEdit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="text"
                android:enabled="false"
                android:ems="10"/>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
            android:id="@+id/vip_layout6"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:orientation="horizontal">

        <TextView
                android:id="@+id/vip_quanXinXText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/vip_quanXinXText"/>

    </LinearLayout>

    <LinearLayout
            android:id="@+id/vip_layout7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <LinearLayout
                android:id="@+id/Vip_couponListViewTitle"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:gravity="center"
                android:orientation="horizontal">

            <TextView
                    android:id="@+id/vip_ticketIdTitle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:gravity="center"
                    android:textColor="#000000"
                    android:text="@string/vip_ticketId"/>

            <TextView
                    android:id="@+id/vip_ticketNameTitle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:textColor="#000000"
                    android:text="@string/vip_ticketName"/>

            <TextView
                    android:id="@+id/vip_ticketMoneyTitle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:textColor="#000000"
                    android:text="@string/vip_ticketMoney"/>

            <TextView
                    android:id="@+id/vip_ticketAmountTitle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:textColor="#000000"
                    android:text="@string/vip_ticketAmount"/>

        </LinearLayout>
        <ListView
                android:id="@+id/Vip_couponListView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:cacheColorHint="#00000000"
                >
        </ListView>

    </LinearLayout>

</LinearLayout>
